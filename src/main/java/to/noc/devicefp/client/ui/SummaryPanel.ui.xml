<ui:UiBinder    xmlns:ui="urn:ui:com.google.gwt.uibinder"
                xmlns:g="urn:import:com.google.gwt.user.client.ui"
                xmlns:df="urn:import:to.noc.devicefp.client.ui"
                >
    <ui:style>
        .uaIcon {
            vertical-align: text-bottom;
        }
    </ui:style>

    <g:HTMLPanel>
        <table>            
            <tr ui:field="publicHostnameRow">
                <td>Public Hostname</td>
                <td>
                    <span ui:field="publicHostnameSpan"/>
                </td>
            </tr>
            <tr ui:field="publicIpRow">
                <td>Public IP (Port)</td>
                <td>
                    <span ui:field="publicIpSpan"/>
                </td>
            </tr>
            <tr ui:field="proxiedHostRow">
                <td>Proxied Host</td>
                <td>
                    <span ui:field="proxiedHostSpan"/>
                </td>
            </tr>
            <tr ui:field="proxiedIpRow">
                <td>Proxied IP</td>
                <td>
                    <span ui:field="proxiedIpSpan"/>
                </td>
            </tr>
            <tr ui:field="lanIpRow">
                <td>LAN IP</td>
                <td>
                    <span ui:field="lanIpSpan"/>
                </td>
            </tr>
            <tr ui:field="publicDnsHostRow">
                <td>Public DNS Host</td>
                <td>
                    <span ui:field="publicDnsHostSpan"/>
                </td>
            </tr>
            <tr ui:field="publicDnsIpRow">
                <td>Public DNS IP</td>
                <td>
                    <span ui:field="publicDnsIpSpan"/>
                </td>
            </tr>
            <tr ui:field="operatingSystemRow">
                <td>Operating System</td>
                <td>
                    <span ui:field="operatingSystemSpan"/>
                </td>
            </tr>
            <tr ui:field="uaBrowserRow">
                <td>UA Browser</td>
                <td>
                    <span ui:field='uaBrowserSpan'/>
                    <img ui:field="uaBrowserIcon" class="{style.uaIcon}" alt="browser icon"/>
                </td>
            </tr>
            <tr ui:field="uaOperatingSystemRow">
                <td>UA Operating System</td>
                <td>
                    <span ui:field='uaOperatingSystemSpan'/>                    
                    <img ui:field="uaOperatingSystemIcon" class="{style.uaIcon}" alt="OS icon"/>                 
                </td>
            </tr>
            <tr ui:field="processorCoresRow">
                <td>Processor Cores</td>
                <td>
                    <span ui:field="processorCoresSpan"/>
                </td>
            </tr>
            <tr ui:field="screenResolutionRow">
                <td>Screen Resolution</td>
                <td>
                    <span ui:field="screenResolutionSpan"/>
                </td>
            </tr>
            <tr ui:field="clientUptimeRow">
                <td>Client Uptime</td>
                <td>
                    <span ui:field="clientUptimeSpan"/>
                </td>
            </tr>
            <tr ui:field="refererRow">
                <td>Referer</td>
                <td>
                    <span ui:field="refererSpan"/>
                </td>
            </tr>
            <tr ui:field="serverStartTimestampRow">
                <td>Server Timestamp</td>
                <td>
                    <df:TimeWithSelectableFormat ui:field="serverStartTimestamp"/>
                </td>
            </tr>
            <tr ui:field="serverEndTimestampRow">
                <td>Server End Time</td>
                <td>
                    <df:TimeWithSelectableFormat ui:field="serverEndTimestamp"/>
                </td>
            </tr>            
            <tr ui:field="clientStartTimestampRow">
                <td>Client Timestamp</td>
                <td>
                    <df:TimeWithSelectableFormat ui:field="clientStartTimestamp"/>
                </td>
            </tr>
            <tr ui:field="clientWindowCloseTimestampRow">
                <td>Window Closed</td>
                <td>
                    <df:TimeWithSelectableFormat ui:field="clientWindowCloseTimestamp"/>
                </td>
            </tr>                                   
        </table>                 
    </g:HTMLPanel>
</ui:UiBinder>
